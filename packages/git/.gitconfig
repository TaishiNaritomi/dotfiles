[user]
	name = taishinaritomi
	email = taishinaritomi@gmail.com
[core]
	editor = code --wait
	ignorecase = false
[merge]
	ff = false
[pull]
	rebase = merges
[alias]
    ch = checkout
    br = branch
    me = merge
[init]
	defaultBranch = main

[credential "https://github.com"]
	helper =
	helper = !/opt/homebrew/bin/gh auth git-credential
[credential "https://gist.github.com"]
	helper =
	helper = !/opt/homebrew/bin/gh auth git-credential
